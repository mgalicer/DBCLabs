<div class="container">
<% if current_user %>

  <div id="all-links">
    <ul>
      <li><%= link_to "All Proposals", proposals_path %></li>
      <li><%= link_to "All Experiments", root_path %></li>
    </ul>
  </div>

  <div id="10-experiments">
  <h1>10 Most Recent Experiments</h1>
  <% @experiments.each_with_index do |e, i| %>
  <ul>

    <li><%= link_to e.title, proposal_experiment_path(e.proposal, e) %></li>
    <% if i == 9 %>
    <br>
      <hr>

      <h1>Older Experiments</h1>
    <% end %>



  </ul>
   <% end %>
 </div>
<% end %>
</div>
